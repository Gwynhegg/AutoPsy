<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AutoPsy.Pages.TablePages.ParameterCreationPage">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
            </Grid.RowDefinitions>

            <Picker x:Name="TypePicker" Title="Вид записи" SelectedIndexChanged="TypePicker_SelectedIndexChanged"></Picker>
            <Picker x:Name="EntityPicker" IsEnabled="False" Grid.Row="1"></Picker>
            <Button x:Name="SaveButton" Grid.Row="2" Clicked="SaveButton_Clicked"></Button>
        </Grid>
    </ContentPage.Content>
</ContentPage>