<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:profile ="clr-namespace:AutoPsy.Pages.ProfilePages"
            xmlns:diary="clr-namespace:AutoPsy.Pages.DiaryPages"
            xmlns:table="clr-namespace:AutoPsy.Pages.TablePages"
            xmlns:report="clr-namespace:AutoPsy.Pages.ReportPages"
            xmlns:root="clr-namespace:AutoPsy.Pages.MainPages"
            xmlns:res="clr-namespace:AutoPsy.Resources"
    x:Class="AutoPsy.Pages.MainPage">
    <!--Pages can be added as references or inline-->
  <profile:ProfilePage Title="{x:Static res:PageTitles.Profile}" />
    <diary:DiaryPage Title="{x:Static res:PageTitles.Diary}" />
    <root:RootPage Title="{x:Static res:PageTitles.Main}"/>
    <table:TablePage Title="{x:Static res:PageTitles.Table}" />
    <report:ReportPage Title="{x:Static res:PageTitles.Results}" />
</TabbedPage>